# Required for AI features
EXPO_PUBLIC_TOOLKIT_URL=https://toolkit.rork.com
EXPO_PUBLIC_RORK_API_BASE_URL=your-backend-url-here

# Supabase (client)
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url-here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Supabase (server)
# Use service role ONLY on the server. Never expose this to the client.
SUPABASE_URL=${EXPO_PUBLIC_SUPABASE_URL}
SUPABASE_SERVICE_ROLE=your-supabase-service-role-key

# Supabase Auth (Google)
EXPO_PUBLIC_GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
EXPO_PUBLIC_GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret
EXPO_PUBLIC_GOOGLE_OAUTH_REDIRECT_URI=myapp://auth-callback

# WHOOP Integration
EXPO_PUBLIC_WHOOP_CLIENT_ID=your-whoop-client-id
EXPO_PUBLIC_WHOOP_CLIENT_SECRET=your-whoop-client-secret

# Garmin Integration
EXPO_PUBLIC_GARMIN_CONSUMER_KEY=your-garmin-consumer-key
EXPO_PUBLIC_GARMIN_CONSUMER_SECRET=your-garmin-consumer-secret

# Strava Integration
EXPO_PUBLIC_STRAVA_CLIENT_ID=your-strava-client-id
EXPO_PUBLIC_STRAVA_CLIENT_SECRET=your-strava-client-secret
EXPO_PUBLIC_STRAVA_REDIRECT_URI=myapp://strava-callback

# Oura Integration
EXPO_PUBLIC_OURA_CLIENT_ID=your-oura-client-id
EXPO_PUBLIC_OURA_CLIENT_SECRET=your-oura-client-secret
EXPO_PUBLIC_OURA_REDIRECT_URI=myapp://oura-callback

# Fitbit Integration
EXPO_PUBLIC_FITBIT_CLIENT_ID=your-fitbit-client-id
EXPO_PUBLIC_FITBIT_CLIENT_SECRET=your-fitbit-client-secret
EXPO_PUBLIC_FITBIT_REDIRECT_URI=myapp://fitbit-callback
